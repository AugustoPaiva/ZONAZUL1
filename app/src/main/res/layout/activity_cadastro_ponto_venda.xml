<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo2"
    tools:context="comviewzonazul.google.httpssites.zonazul.pontovenda.gui.CadastroPontoVendaActivity">

    <EditText
        android:id="@+id/editnome_estabelecimento"
        android:layout_width="230dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Nome do estabelecimento"
        android:textColorHint="@color/colorPrimaryDark"
        android:background="@color/colorAccent2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <EditText
        android:id="@+id/txt_cep"
        android:layout_width="230dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Cep"
        android:textColorHint="@color/colorPrimaryDark"
        android:background="@color/colorAccent2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/editCNPJ" />

    <EditText
        android:id="@+id/txt_cidade"
        android:layout_width="230dp"
        android:layout_height="42dp"
        android:background="@color/colorAccent2"
        android:ems="10"
        android:hint="Cidade"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimaryDark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_numero" />

    <EditText
        android:id="@+id/txt_numero"
        android:layout_width="230dp"
        android:layout_height="42dp"
        android:background="@color/colorAccent2"
        android:ems="10"
        android:hint="NÃºmero"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimaryDark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_cep" />

    <EditText
        android:id="@+id/txt_email"
        android:layout_width="230dp"
        android:layout_height="42dp"
        android:background="@color/colorAccent2"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimaryDark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_complemento" />

    <EditText
        android:id="@+id/txt_complemento"
        android:layout_width="230dp"
        android:layout_height="42dp"
        android:background="@color/colorAccent2"
        android:ems="10"
        android:hint="Complemento"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimaryDark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_cidade" />

    <EditText
        android:id="@+id/editCNPJ"
        android:layout_width="230dp"
        android:layout_height="42dp"
        android:background="@color/colorAccent2"
        android:ems="10"
        android:hint="CNPJ"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimaryDark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/editnome_estabelecimento" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastro Ponto de Venda"
        android:textColor="@color/colorAccent2"
        android:textSize="28sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.085" />

    <Button
        android:id="@+id/button13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salvar"
        android:onClick="cadastroPontoVenda"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="88dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_email" />

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voltar"
        android:onClick="voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button13"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.605"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_email" />

</android.support.constraint.ConstraintLayout>
